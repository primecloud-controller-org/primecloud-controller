    elif "listService" == method:
        if "0" == resultDict["result"]:
            retData = resultDict["data"]
            resultDataList = retData.split("&&")
            print("COMPONENT_TYPE_NO\tCOMPONENT_TYPE_NAME\tSELECTABLE")
